                "terraform-provider-incus",
                "terraform-provider-incus_v@VERSION@"
            ]
        ],
        "install_targets": [
            [
                "terraform-provider-incus_v@VERSION@",
                "usr/share/terraform/plugins/registry.opentofu.org/lxc/incus/@VERSION@/linux_@ARCH@/"
            ]
        ],
        "clean_targets": [
            "usr/share/terraform/plugins/registry.opentofu.org/lxc/incus/"
        ]
    },
